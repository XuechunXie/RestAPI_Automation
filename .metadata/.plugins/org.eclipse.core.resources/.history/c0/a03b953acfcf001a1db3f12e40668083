package pojo;

public class Location {
	private double lat;
	private double lng;
	
	public void setLatitude(double lat) {
		this.lat = lat;
	}
	
	public void setLongitude(double lng) {
		this.lng = lng;
	}
	
	public double getLatitude() {
		return lat;
	}
	
	public double getLongitude() {
		return lng;
	}


}
